<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:toolbar="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/color_default_background"
    tools:context="com.haloai.hud.androidendpoint.views.navi.HudAddCollectionFragment">

    <android.support.v7.widget.Toolbar
        android:id="@+id/tipsToolbar"
        android:layout_height="@dimen/common_toolbar_height"
        android:layout_width="match_parent"
        toolbar:titleMargins="@dimen/margin_toolbar_title"
        toolbar:title="@string/app_name"
        toolbar:titleTextColor="@color/color_toolbar_titleText"
        toolbar:navigationIcon="@drawable/common_icon_arrow_back_24px"
        android:background="@color/color_toolbar_background">

    </android.support.v7.widget.Toolbar>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/color_default_foreground"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/margin_common_sizeTop">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:padding="@dimen/padding_common_imageview"
            android:layout_marginLeft="@dimen/margin_common_sizeLeft"
            android:maxHeight="@dimen/maxsize_icon_imageview_heignt"
            android:maxWidth="@dimen/maxsize_icon_imageview_width"
            android:src="@drawable/tips_icon_home"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:padding="@dimen/padding_common_textview"
            android:text="家庭地址"
            android:textSize="@dimen/font_common_title"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/padding_common_textview"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:text="(点击设置)"
            android:textSize="@dimen/font_common_subtitle"/>


    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="2px"
        android:paddingLeft="@dimen/margin_common_line"
        android:paddingRight="@dimen/margin_common_line"
        android:background="@color/darkgrey"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/color_default_foreground"
        android:orientation="horizontal">
        <ImageView
            android:id="@+id/id_home_star_imageview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/padding_common_imageview"
            android:layout_centerVertical="true"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/margin_common_sizeLeft"
            android:maxHeight="@dimen/maxsize_icon_imageview_heignt"
            android:maxWidth="@dimen/maxsize_icon_imageview_width"
            android:src="@drawable/common_icon_blue_star"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/id_home_star_imageview"
            android:layout_gravity="center_vertical"
            android:layout_centerVertical="true"
            android:gravity="center_vertical"
            android:padding="@dimen/padding_common_textview"
            android:text="深圳大学"
            android:textSize="@dimen/font_common_title"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_centerVertical="true"
            android:padding="@dimen/padding_common_imageview"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/margin_common_sizeRight"
            android:maxHeight="@dimen/maxsize_icon_imageview_heignt"
            android:maxWidth="@dimen/maxsize_icon_imageview_width"
            android:src="@drawable/conmon_icon_delete"/>


    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="2px"
        android:paddingLeft="@dimen/margin_common_line"
        android:paddingRight="@dimen/margin_common_line"
        android:background="@color/darkgrey"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/color_default_foreground"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/margin_common_sizeTop">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:padding="@dimen/padding_common_imageview"
            android:layout_marginLeft="@dimen/margin_common_sizeLeft"
            android:maxHeight="@dimen/maxsize_icon_imageview_heignt"
            android:maxWidth="@dimen/maxsize_icon_imageview_width"
            android:src="@drawable/tips_icon_company"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:padding="@dimen/padding_common_textview"
            android:text="家庭地址"
            android:textSize="@dimen/font_common_title"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/padding_common_textview"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:text="(点击设置)"
            android:textSize="@dimen/font_common_subtitle"/>


    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="2px"
        android:paddingLeft="@dimen/margin_common_line"
        android:paddingRight="@dimen/margin_common_line"
        android:background="@color/darkgrey"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/color_default_foreground"
        android:orientation="horizontal">
        <ImageView
            android:id="@+id/id_company_star_imageview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/padding_common_imageview"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/margin_common_sizeLeft"
            android:maxHeight="@dimen/maxsize_icon_imageview_heignt"
            android:maxWidth="@dimen/maxsize_icon_imageview_width"
            android:src="@drawable/common_icon_blue_star"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/id_company_star_imageview"
            android:layout_centerVertical="true"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:padding="@dimen/padding_common_textview"
            android:text="深圳大学"
            android:textSize="@dimen/font_common_title"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:padding="@dimen/padding_common_imageview"
            android:layout_marginRight="@dimen/margin_common_sizeRight"
            android:maxHeight="@dimen/maxsize_icon_imageview_heignt"
            android:maxWidth="@dimen/maxsize_icon_imageview_width"
            android:src="@drawable/conmon_icon_delete"/>

    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="2px"
        android:layout_margin="@dimen/margin_common_size"
        android:paddingLeft="@dimen/margin_common_line"
        android:paddingRight="@dimen/margin_common_line"
        android:background="@color/darkgrey"/>

    <ListView
        android:id="@+id/history_searched_listview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/color_default_foreground">

    </ListView>



</LinearLayout>
